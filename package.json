{
    "version": "1.1.0",
    "name": "afrijour",
    "description": "African Journals Visibility Project",
    "author": "dkurui",
    "keywords": [],
    "license": "MIT", 
    "private": "false",
    "homepage": ".",
    

   
    "scripts": {
      "build": "NODE_ENV=production run-s build:*",
      "build:css": "postcss styles/tailwind.css -o src/assets/styles.css",
      "build:vite": "vite build",
      "deploy": "gh-pages -d dist",
      "dev": "run-p dev:*",
      "dev:css": "postcss styles/tailwind.css -o src/assets/styles.css --watch",
      "dev:vite": "vite",
      "format": "prettier --check .",
      "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
      "lint:fix": "eslint . --fix",
      "predeploy": "$npm_execpath run build",
      "preview": "vite preview",
      "start": "$npm_execpath run dev",
      "typecheck": "tsc --noEmit",
      "validate": "run-p lint typecheck"
    },
    "dependencies": {
      "@heroicons/react": "^2.1.1",
      "@material-tailwind/react": "^2.1.9",
      "clsx": "^2.1.0",
      "flowbite": "^2.3.0",
      "flowbite-react": "^0.7.2",
      "gh-pages": "^6.1.1",
      "heroicons": "^2.1.1",
      "postcss-import": "^16.0.0",
      "radix-ui": "^1.0.1",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-icons": "^5.0.1",
      "react-pro-sidebar": "^1.1.0",
      "react-router-dom": "^6.22.1",
      "react-slick": "^0.30.2",
      "scrollreveal": "^4.0.9",
      "slick-carousel": "^1.8.1",
      "styled-components": "^6.1.8",
      "tailwind-merge": "^2.2.0",
      "tiny-invariant": "^1.3.1"
    },
    "devDependencies": {
      "@remix-run/eslint-config": "^2.5.1",
      "@tailwindcss/forms": "^0.5.7",
      "@tailwindcss/typography": "^0.5.10",
      "@types/eslint": "^8.56.2",
      "@types/node": "^20.11.5",
      "@types/react": "^18.2.48",
      "@types/react-dom": "^18.2.18",
      "@types/scrollreveal": "^0.0.11",
      "@vitejs/plugin-react": "^4.2.1",
      "autoprefixer": "^10.4.17",
      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "npm-run-all": "^4.1.5",
      "postcss": "^8.4.35",
      "postcss-cli": "^11.0.0",
      "prettier": "^3.2.4",
      "prettier-plugin-tailwindcss": "^0.5.11",
      "tailwindcss": "^3.4.1",
      "typescript": "^5.3.3",
      "vite": "^5.0.12"
    }
